# Frontend environment variables for Vite
# Copy this file to .env.local for local development
# For production, set these in Cloudflare Pages → Settings → Environment variables

# Supabase — get from https://app.supabase.com/project/<your-project>/settings/api
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Cloudflare Turnstile — get sitekey from https://dash.cloudflare.com/?to=/:account/turnstile
VITE_TURNSTILE_SITE_KEY=your-turnstile-site-key-here
